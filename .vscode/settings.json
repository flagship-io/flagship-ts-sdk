{
  "eslint.validate": ["javascript", "typescript"],
  "cSpell.words": [
    "allowlist",
    "camelcase",
    "Deduplication",
    "Deno",
    "resave",
    "sprintf",
    "targetings",
    "unauthenticate"
  ],
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": null
  },
  "deno.enable": false,
}
