{
  "eslint.validate": ["javascript", "typescript"],
  "cSpell.words": ["Deno", "allowlist", "sprintf"],
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "[javascript]": {
    "editor.defaultFormatter": null
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  }
}
