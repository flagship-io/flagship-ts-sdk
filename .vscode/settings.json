{
  "eslint.validate": ["javascript", "typescript"],
  "cSpell.words": [
    "allowlist",
    "camelcase",
    "Deno",
    "resave",
    "sprintf",
    "targetings",
    "unauthenticate"
  ],
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "[javascript]": {
    "editor.defaultFormatter": null
  },
  "[typescript]": {
    "editor.defaultFormatter": null
  }
  // "deno.enable": true,
  // "deno.lint": true,
  // "deno.unstable": true
}
